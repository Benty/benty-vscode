{
  "name": "benty-vscode",
  "version": "0.0.1",
  "publisher": "benty",
  "description": "A Benty-styled dark UI theme",
  "type": "module",
  "engines": {
    "vscode": "^1.57.0"
  },
  "scripts": {
    "build": "pinecone",
    "watch": "pinecone --watch",
    "format": "prettier --write .",
    "release": "np --no-publish --no-tests",
    "version": "npm run build && npm run format"
  },
  "displayName": "Benty VSCode",
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Benty",
        "uiTheme": "vs-dark",
        "path": "./themes/benty-color-theme.json"
      },
      {
        "label": "Benty Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/benty-dark-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "@rose-pine/palette": "^2.0.0-4",
    "chroma-js": "^2.4.2",
    "np": "^7.6.1",
    "pinecone-cli": "^4.0.0",
    "prettier": "^2.5.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "useTabs": true
  }
}
