{
  "name": "benty-vscode",
  "description": "A Benty-styled dark UI theme",
  "version": "1.0.1",
  "publisher": "Benty",
  "icon": "assets/icon.png",
  "license": "MIT",
  "type": "module",
  "galleryBanner": {
    "color": "#0C1A23",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Benty/benty-vscode"
  },
  "engines": {
    "vscode": "^1.57.0"
  },
  "scripts": {
    "build": "pinecone",
    "watch": "pinecone --watch",
    "format": "prettier --write .",
    "release": "np --no-publish --no-tests",
    "version": "npm run build && npm run format",
    "bundle": "vsce package",
    "publish": "vsce publish"
  },
  "displayName": "Benty - VSCode",
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Benty",
        "uiTheme": "vs-dark",
        "path": "./themes/benty-color-theme.json"
      },
      {
        "label": "Benty Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/benty-dark-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "@rose-pine/palette": "^2.0.0-4",
    "@vscode/vsce": "^2.18.0",
    "np": "^7.6.1",
    "pinecone-cli": "^4.0.0",
    "prettier": "^2.5.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "useTabs": true
  }
}
