	"tokenColors": [
		{
			"scope": ["comment"],
			"settings": {
				"foreground": "$muted",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["constant"],
			"settings": {
				"foreground": "$gold"
			}
		},
		{
			"scope": ["constant.numeric", "constant.language"],
			"settings": {
				"foreground": "$pink"
			}
		},
		{
			"scope": ["entity.name"],
			"settings": {
				"foreground": "$lightgreen"
			}
		},
		{
			"scope": [
				"entity.name.section",
				"entity.name.tag",
				"entity.name.namespace",
				"entity.name.type"
			],
			"settings": {
				"foreground": "$green"
			}
		},
		{
			"scope": ["entity.other.attribute-name", "entity.other.inherited-class"],
			"settings": {
				"foreground": "$orange",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["invalid"],
			"settings": {
				"foreground": "$yellow"
			}
		},
		{
			"scope": ["invalid.deprecated"],
			"settings": {
				"foreground": "$subtle"
			}
		},
		{
			"scope": ["keyword"],
			"settings": {
				"foreground": "$gold"
			}
		},
		{
			"scope": ["markup.inserted.diff"],
			"settings": {
				"foreground": "$foam"
			}
		},
		{
			"scope": ["markup.deleted.diff"],
			"settings": {
				"foreground": "$yellow"
			}
		},
		{
			"scope": ["meta.diff.range"],
			"settings": {
				"foreground": "$purple"
			}
		},
		{
			"scope": ["meta.tag", "meta.brace"],
			"settings": {
				"foreground": "$text"
			}
		},
		{
			"scope": ["meta.import", "meta.export"],
			"settings": {
				"foreground": "$gold"
			}
		},
		{
			"scope": "meta.directive.vue",
			"settings": {
				"foreground": "$purple",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "meta.objectliteral.js",
			"settings": {
				"foreground": "$cyan"
			}
		},
		{
			"scope": "meta.property-name.css",
			"settings": {
				"foreground": "$foam"
			}
		},
		{
			"scope": "meta.property-value.css",
			"settings": {
				"foreground": "$lime"
			}
		},
		{
			"scope": "meta.tag.other.html",
			"settings": {
				"foreground": "$subtle"
			}
		},
		{
			"scope": "meta.tag.metadata.doctype.html",
			"settings": {
				"foreground": "$subtle"
			}
		},
		{
			"scope": ["punctuation"],
			"settings": {
				"foreground": "$text"
			}
		},
		{
			"scope": ["punctuation.accessor"],
			"settings": {
				"foreground": "$text"
			}
		},
		{
			"scope": ["punctuation.definition.string"],
			"settings": {
				"foreground": "$text"
			}
		},
		{
			"scope": ["punctuation.definition.tag"],
			"settings": {
				"foreground": "$text"
			}
		},
		{
			"scope": ["punctuation.definition.comment"],
			"settings": {
				"foreground": "$muted",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["storage.type", "storage.modifier"],
			"settings": {
				"foreground": "$gold"
			}
		},
		{
			"scope": "storage.type.function.js",
			"settings": {
				"foreground": "$purple"
			}
		},
		{
			"scope": ["string"],
			"settings": {
				"foreground": "$lime"
			}
		},
		{
			"scope": ["support"],
			"settings": {
				"foreground": "$foam"
			}
		},
		{
			"scope": ["support.constant"],
			"settings": {
				"foreground": "$lime"
			}
		},
		{
			"scope": ["support.function"],
			"settings": {
				"foreground": "$yellow",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["variable"],
